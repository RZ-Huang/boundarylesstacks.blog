---
import Centered from '@/layouts/Centered.astro';
import Prose from '@/components/Prose.astro';

import type { BaseProps } from '@/layouts/Base.astro';

export interface Props extends BaseProps {}

const props = Astro.props;
---

<Centered {...props}>
